{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "tidy": {},
    "build": {
      "dependsOn": ["^build", "db:generate"],
      "inputs": ["./src/**"],
      "outputs": ["./dist/**"]
    },
    "db:generate": {
      "inputs": ["./prisma/schema.prisma"],
      "outputs": ["./src/db.generated.ts"]
    }
  }
}
