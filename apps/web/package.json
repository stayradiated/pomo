{
	"name": "web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "POMO_DIR=./tmp vite dev",
		"build:vite": "vite build",
		"build": "rm -rf dist && vite build && node ./scripts/build.mjs && cp -r ./build/client ./dist/client",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"tidy": "prettier --write --plugin-search-dir=. ./src"
	},
	"dependencies": {
		"@stayradiated/error-boundary": "4.1.0",
		"@stayradiated/pomo-core": "workspace:*",
		"@stayradiated/pomo-doc": "workspace:*",
		"@svelte-put/shortcut": "2.0.0",
		"date-fns": "2.30.0",
		"pollen": "0.0.5",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"svelte-select": "5.6.1",
		"zod": "3.21.4",
		"zod-form-data": "2.0.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "1.2.4",
		"@sveltejs/kit": "1.21.0",
		"@vercel/ncc": "0.36.1",
		"date-fns-tz": "2.0.0",
		"svelte": "4.0.1",
		"svelte-check": "3.4.4",
		"tslib": "2.6.0",
		"typescript": "5.1.6",
		"vite": "4.3.9"
	},
	"type": "module",
	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": true,
		"semi": false,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "all",
		"useTabs": false
	}
}
