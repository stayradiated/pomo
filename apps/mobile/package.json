{
  "name": "mobile",
  "version": "0.62.0",
  "private": true,
  "scripts": {
    "dev": "POMO_DIR=./tmp vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tidy": "biome format --write; eslint . --config eslint.config.js",
    "pollen": "pollen",
    "knip": "knip"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@eslint/js": "9.39.2",
    "@sveltejs/adapter-node": "5.2.12",
    "@sveltejs/kit": "2.17.3",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "eslint-plugin-svelte": "3.13.1",
    "globals": "16.0.0",
    "knip": "5.45.0",
    "pollen-css": "5.0.1",
    "prettier": "3.5.2",
    "prettier-plugin-svelte": "3.3.3",
    "svelte": "5.20.5",
    "svelte-check": "4.1.4",
    "typescript": "5.7.3",
    "typescript-eslint": "8.51.0",
    "vite": "6.2.0",
    "vitest": "3.0.7"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "1.6.13",
    "@stayradiated/error-boundary": "4.3.0",
    "@stayradiated/pomo-core": "workspace:*",
    "@stayradiated/pomo-doc": "workspace:*",
    "@svelte-put/shortcut": "4.1.0",
    "chrono-node": "2.7.8",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "lib0": "0.2.99",
    "normalize.css": "8.0.1",
    "p-debounce": "4.0.0",
    "p-queue": "8.1.0",
    "svelte-awesome-color-picker": "3.1.4",
    "svelte-select": "5.8.3",
    "zod": "3.24.2",
    "zod-form-data": "2.0.5"
  },
  "knip": {
    "svelte": {
      "entry": [
        "./src/routes/**/+{layout,page,server,error}{,.server}.{svelte,ts}",
        "./svelte.config.js"
      ],
      "project": [
        "./src/**/*.ts"
      ]
    },
    "paths": {
      "$lib/*": [
        "src/lib/*"
      ],
      "$app/*": [
        "node_modules/@sveltejs/kit/src/runtime/app/*"
      ],
      "$env/*": [
        "node_modules/@sveltejs/kit/src/runtime/env/*"
      ]
    },
    "ignore": [
      "pollen.config.js"
    ],
    "ignoreDependencies": [],
    "ignoreBinaries": [
      "sed"
    ],
    "ignoreExportsUsedInFile": true
  }
}
